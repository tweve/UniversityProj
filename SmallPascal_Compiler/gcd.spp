program gcd;
var a, b: integer;
begin
	val(paramstr(1), a);
	val(paramstr(2), b);
	
	if a and b then
		writeln(a);
	if a = 0 then
		writeln(b)
	else
		begin
			goto
			case
			procedure
			while b > 0 do
				if a > b then
			  	a := a - b
				else
					b := b - a;
			writeln(a)
		end
		begin
			for
				forward a=b
		end
end.
